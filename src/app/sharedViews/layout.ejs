<!DOCTYPE html>
<html lang="en" class="govuk-template govuk-template--rebranded">
<head>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta name="theme-color" content="#1d70b8">
    <meta name="keywords" content="DfE Sign-In, Secure Access, DfE Login, Key to Success, Analyse School Performance, Teacher services, Teaching jobs, Trainee Teacher Portal, Collect, Get Information About Schools, Information exchange, Post 16 portal, School to School, Publish teacher training courses">
    <meta name="description" content ="Use DfE Sign-in to login to services including Analyse School Performance, COLLECT, Key to Success and other services previously accessed through Secure Access.">

    <% if((locals.validationMessages && Object.keys(locals.validationMessages).length !==0) || 
           (locals.validations && Object.keys(locals.validations).length !==0 )) { %>
        <title>
           Error: <%=locals.title || (locals.app ? locals.app.title : null) || 'DfE Sign-in' %>
        </title>
    <% } else { %>
        <title>
            <%=locals.title || locals.app.title || 'DfE Sign-in' %>
        </title>
    <% } %>         

    <link rel="stylesheet" href="<%= locals.urls.assets %>/css/main.css">

    <link rel="icon" sizes="48x48" href="<%= locals.urls.assets %>/assets/rebrand/images/favicon.ico">

    <link rel="icon" sizes="any" href="<%= locals.urls.assets %>/assets/rebrand/images/favicon.svg" type="image/svg+xml">
    <link rel="mask-icon" href="<%= locals.urls.assets %>/assets/rebrand/images/govuk-icon-mask.svg" color="#1d70b8">
    <link rel="apple-touch-icon" href="<%= locals.urls.assets %>/assets/rebrand/images/govuk-icon-180.png">
    <link rel="manifest" href="<%= locals.urls.assets %>/assets/rebrand/manifest.json">

    <meta property="og:image" content="<%= locals.urls.assets %>/images/opengraph-image.png">
</head>

<body class="govuk-template__body">

<script>document.body.className += ' js-enabled' + ('noModule' in HTMLScriptElement.prototype ? ' govuk-frontend-supported' : '');</script>

<a href="#content" class="govuk-skip-link">Skip to main content</a>

<div class="wrapper">
    <%if(app.environmentBannerMessage) {%>
    <div class="environment-banner">
        <div class="govuk-body-s"><%-app.environmentBannerMessage%></div>
    </div>
    <%}%>

    <header class="govuk-header " data-module="govuk-header">
        <div class="govuk-header__container govuk-width-container">
            <div class="govuk-header__logo">
                <a href="https://www.gov.uk/government/organisations/department-for-education" class="govuk-header__link govuk-header__link--homepage">
                  <img src="<%= locals.urls.assets %>/assets/images/department-for-education_white.png" alt="Department for Education" class="govuk-header__logotype">
                </a>
            </div>
            <% if (!locals.hideSignOut) { %>
            <div class="app-auth-action">
                <a class="app-auth-action__link" href="/signout">
                    Sign out
                </a>
            </div>
            <% } %>
        </div>
    </header>

    <section aria-label="Service information" class="govuk-service-navigation" data-module="govuk-service-navigation">
        <div class="govuk-width-container">
            <div class="govuk-service-navigation__container">
                <span class="govuk-service-navigation__service-name">
                    <a href="/users" class="govuk-service-navigation__link">
                        DfE Sign-In support console
                    </a>
                </span>

                <% if (locals.isLoggedIn) { %>
                <nav aria-label="Menu" class="govuk-service-navigation__wrapper">
                    <button type="button" class="govuk-service-navigation__toggle govuk-js-service-navigation-toggle" aria-controls="navigation" hidden>
                    Menu
                    </button>
                    <ul id="navigation" class="govuk-service-navigation__list " aria-label="Navigation menu">
                        <li class="govuk-service-navigation__item">
                            <a class="govuk-service-navigation__link" href="<%=locals.urls.profile%>">
                                My Profile
                            </a>
                        </li>
                        <li class="govuk-service-navigation__item <%= locals.currentPage === 'users' ? 'govuk-service-navigation__item--active' : '' %>">
                            <a class="govuk-service-navigation__link" href="/users">
                                Users
                            </a>
                        </li>
                        <li class="govuk-service-navigation__item <%= locals.currentPage === 'organisations' ? 'govuk-service-navigation__item--active' : '' %>">
                            <a class="govuk-service-navigation__link" href="/organisations">
                                Organisations
                            </a>
                        </li>
                        <% if (locals.isServiceCreator) { %>
                        <li class="govuk-service-navigation__item <%= locals.currentPage === 'services' ? 'govuk-service-navigation__item--active' : '' %>">
                            <a class="govuk-service-navigation__link" href="/services/choose-type">
                                Create New Service
                            </a>
                        </li>
                        <% } %>
                    </ul>
                </nav>
                <% } %>
            </div>
        </div>
    </section>

    <main id="content" class="govuk-!-margin-bottom-9">
        <div class="govuk-width-container">
            <% if (locals.backLink !== undefined) { %>
                <% if (locals.backLink === true) { %>
                    <span class="js-back-link"></span>
                <% } else { %>
                    <a href="<%=locals.backLink%>" class="govuk-back-link">Back</a>
                <% } %>
            <% } %>
            

            <% if (locals.validationMessages !== undefined) { %>
                <% const validationKeys = Object.keys(locals.validationMessages)  %>
                <% if (validationKeys.length > 0) { %>
                    <div class="govuk-error-summary govuk-!-margin-top-6" aria-labelledby="error-summary-title" role="alert" tabindex="-1" data-module="govuk-error-summary">
                        <h2 class="govuk-error-summary__title" id="error-summary">
                            There is a problem
                        </h2>
                        <div class="govuk-error-summary__body">
                            <ul class="govuk-list govuk-error-summary__list">
                            <% for (const prop in locals.validationMessages) { %>
                                <li><a class="govuk-link" href="#<%=prop%>"><%-locals.validationMessages[prop]%></a></li>
                                <% } %>
                            </ul>
                        </div>
                    </div>
                <% } %>
            <% } %>
        </div>
        <%- body %>

    </main>

</div>

<footer class="govuk-footer">
