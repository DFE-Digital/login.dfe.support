{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Support",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/web.js",
            "env": {
                "settings": "config/login.dfe.support.dev.json",  
                "NODE_TLS_REJECT_UNAUTHORIZED": "0"
            },
            "outputCapture": "std"
        }
    ]
}