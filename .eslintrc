{
  "extends": "airbnb-base",
  "rules": {
    "quotes": [2, "single"],
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "prefer-rest-params": "off",
    "prefer-spread": "off",
    "strict": "off",
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "max-len": 0
  },
  "plugins": ["jest"]
}
